    "playwright": "^1.40.0",
    "reflect-metadata": "^0.2.0",
    "tsyringe": "^4.8.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/node": "^20.0.0",
    "@types/node-cron": "^3.0.0",
    "prisma": "^5.7.0",
    "ts-node": "^10.9.0",
    "tsconfig-paths": "^4.2.0",
